# prefix
prefix=["/home/clinares/lab/ipc2011-data/"];

# problems
nbproblems = 20;
prefix_problem = ["problems/"];
problems = ["problem"] * nbproblems + (["%03d"] * nbproblems % range (0, nbproblems));
pproblems = prefix * nbproblems + prefix_problem * nbproblems + problems + [".pddl"] * nbproblems;

# domains
nbdomains = 2;
prefix_domain = ["domain/"];
domains = ['spanner', 'blocksworld'];
pdomains = prefix * nbdomains + prefix_domain * nbdomains + domains + [".pddl"] * nbdomains;

# exec commands
exec ['plan %s %s plan.soln'] * nbdomains * nbproblems % pdomains @ pproblems;
